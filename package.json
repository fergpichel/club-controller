{
  "name": "club-controller",
  "version": "1.0.0",
  "description": "Financial management app for sports clubs",
  "productName": "Grootter Finance",
  "author": "Grootter Finance Team",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:mem": "NODE_OPTIONS='--max-old-space-size=4096' quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./src",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "emulators": "firebase emulators:start --project demo-club-controller",
    "emulators:clean": "firebase emulators:start --project demo-club-controller --import=./emulator-data --export-on-exit=./emulator-data",
    "dev:firebase": "concurrently \"npm run emulators\" \"npm run dev\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@quasar/extras": "^1.16.9",
    "@vueuse/core": "^10.7.2",
    "chart.js": "^4.4.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "date-fns": "^3.3.1",
    "firebase": "^10.8.0",
    "pinia": "^2.1.7",
    "quasar": "^2.14.3",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.8.0",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.4",
    "typescript": "~5.3.3",
    "vite-plugin-checker": "^0.6.2"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
